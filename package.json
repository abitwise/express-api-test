{
  "author": "Olger Oeselg <olger.oeselg@gmail.com> (http://www.github.com/abitwise)",
  "name": "express-api-test",
  "description": "Express.js API Testing Utility",
  "version": "1.0.6",
  "homepage": "https://github.com/abitwise/express-api-test",
  "bugs": {
    "url": "https://github.com/abitwise/express-api-test/issues"
  },
  "contributors": [
    {
      "name": "Olger Oeselg",
      "email": "olger.oeselg@gmail.com",
      "url": "https://github.com/abitwise"
    }
  ],
  "license": "ISC",
  "keywords": [
    "express",
    "api",
    "test",
    "mock",
    "stub",
    "unit",
    "router",
    "mocha",
    "sinon"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/abitwise/express-api-test.git"
  },
  "main": "./lib/express-api-test.js",
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "chai": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "eslint": "^5.6.1",
    "http-status-codes": "^1.3.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "standard": "^12.0.1"
  },
  "scripts": {
    "build": "npx babel ./src/express-api-test.js --out-file ./lib/express-api-test.js",
    "test": "npx mocha --recursive --timeout 30000 ./test/loader.js ./test/*.test.js",
    "lint": "npx eslint src test",
    "coverage": "npx istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive --timeout 30000 ./test/loader.js ./test/*.test.js",
    "docs": "npx jsdoc2md src/express-api-test.js > ./docs/API.md"
  },
  "files": [
    "lib",
    "docs"
  ]
}
